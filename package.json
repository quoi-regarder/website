    "@nuxt/ui": "3.0.0-beta.2",
    "@nuxtjs/i18n": "^9.1.4",
    "@nuxtjs/seo": "^2.1.1",
    "@pinia/nuxt": "^0.9.0",
    "@vueuse/nuxt": "^12.5.0",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
